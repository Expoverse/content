{
	"settings": {
		"bullet": "-"
	},
	"plugins": [
		[
			"remark-frontmatter",
			{
				"type": "yaml",
				"marker": "-"
			},
			"remark-parse"
		],
		[
			"remark-slug"
		],
		[
			"remark-autolink-headings",
			{
				"behavior": "append",
				"linkProperties": {
					"type": "element",
					"tagName": "span",
					"properties": {
						"className": [
							"icon",
							"icon-link"
						]
					}
				}
			}
		],
		[
			"remark-html",
			{
				"sanitize": false
			}
		]
	]
}
